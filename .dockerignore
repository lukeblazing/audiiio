# Ignore node_modules in both Client and Server
Client/node_modules
Server/node_modules

# Ignore build output of React app (Webpack build)
Client/build
Client/dist

# Ignore temporary files and logs
**/*.log
**/*.tmp
**/*.lock
**/*.pid

# Ignore test coverage reports
Client/coverage
Server/coverage

# Ignore environment variable files
.env
.env.local
.env.development
.env.production
.env.test

# Ignore MacOS and other system-specific files
.DS_Store
Thumbs.db

